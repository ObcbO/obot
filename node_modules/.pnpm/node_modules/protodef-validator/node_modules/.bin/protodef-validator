#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/m1708/Desktop/BOT/mineflayer/node_modules/.pnpm/registry.npmmirror.com+protodef-validator@1.3.1/node_modules/protodef-validator/node_modules:/mnt/c/Users/m1708/Desktop/BOT/mineflayer/node_modules/.pnpm/registry.npmmirror.com+protodef-validator@1.3.1/node_modules:/mnt/c/Users/m1708/Desktop/BOT/mineflayer/node_modules/.pnpm/node_modules:/mnt/c/Users/m1708/Desktop/BOT/mineflayer/node_modules:/mnt/c/Users/m1708/Desktop/BOT/node_modules:/mnt/c/Users/m1708/Desktop/node_modules:/mnt/c/Users/m1708/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/m1708/Desktop/BOT/mineflayer/node_modules/.pnpm/registry.npmmirror.com+protodef-validator@1.3.1/node_modules/protodef-validator/node_modules:/mnt/c/Users/m1708/Desktop/BOT/mineflayer/node_modules/.pnpm/registry.npmmirror.com+protodef-validator@1.3.1/node_modules:/mnt/c/Users/m1708/Desktop/BOT/mineflayer/node_modules/.pnpm/node_modules:/mnt/c/Users/m1708/Desktop/BOT/mineflayer/node_modules:/mnt/c/Users/m1708/Desktop/BOT/node_modules:/mnt/c/Users/m1708/Desktop/node_modules:/mnt/c/Users/m1708/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
